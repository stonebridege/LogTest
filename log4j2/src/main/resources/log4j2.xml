<?xml version="1.0" encoding="UTF-8" ?>
<!--<Configuration status="debug">-->
<Configuration>
    <!--  配置appender-->
    <Appenders>
        <Console name="consoleAppender" target="SYSTEM_ERR"/>
    </Appenders>
    <!--  配置logger-->
    <Loggers>
        <!--  配置rootLogger-->
        <Root level="trace">
            <!--  引用Appender-->
            <AppenderRef ref="consoleAppender"/>
        </Root>
    </Loggers>
</Configuration>